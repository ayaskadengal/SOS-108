<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LabelText">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginTop">12dp</item>
    </style>

    <style name="InputField">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/edittext_background</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_marginBottom">8dp</item>
    </style>

    <style name="splash_screen_title">
        <item name="android:fontFamily">@font/quicksand_light</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="splash_screen_title_bold">
        <item name="android:fontFamily">@font/quicksand_bold</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Select_lang_style">
        <item name="android:fontFamily">@font/quicksand_bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">20sp</item>

    </style>

    <style name="select_lang_radio">
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
        <item name="buttonTint">@color/radio_bt</item>
    </style>

    <style name="next_button">
        <item name="android:layout_width">195dp</item>
        <item name="android:layout_height">58dp</item>
        <item name="android:backgroundTint">@color/primary_red</item>
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="normal_txt">
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>



    </style>

    <style name="emoji">
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>



    </style>




    <style name="normal_txt_bold">
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>

    </style>

    <style name="normal_title">
        <item name="android:fontFamily">@font/quicksand_bold</item>
        <item name="android:textColor">@color/primary_red</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="normal_title_blue">
        <item name="android:fontFamily">@font/quicksand_bold</item>
        <item name="android:textColor">@color/secondary_blue</item>
        <item name="android:textSize">24sp</item>
    </style>
    <style name="normal_title_cyan">
        <item name="android:fontFamily">@font/quicksand_bold</item>
        <item name="android:textColor">@color/secondary_cyan</item>
        <item name="android:textSize">24sp</item>
    </style>
    <style name="normal_title_cyan_20">
        <item name="android:fontFamily">@font/quicksand_bold</item>
        <item name="android:textColor">@color/secondary_cyan</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="normal_title_non_bold">
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/primary_red</item>
        <item name="android:textSize">24sp</item>
    </style>


    <style name="forgot_password" parent="normal_txt">
        <item name="android:textColor">@color/primary_red</item>
    </style>


    <style name="normal_bt">
        <item name="android:layout_width">270dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:backgroundTint">@color/primary_red</item>
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="normal_bt_add_sleep">
        <item name="android:layout_width">150dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:backgroundTint">@color/primary_red</item>
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="normal_edit_txt">
        <item name="android:layout_width">270dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/rounded_edittxt</item>
        <item name="android:ems">10</item>

        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/edittxt_hint_grey</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="address_edit_txt">
        <item name="android:layout_width">270dp</item>
        <item name="android:layout_height">100dp</item>
        <item name="android:background">@drawable/rounded_edittxt</item>
        <item name="android:ems">10</item>

        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/edittxt_hint_grey</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="i_agree_radio">
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/primary_red</item>
        <item name="android:textSize">15sp</item>
        <item name="buttonTint">@color/radio_bt</item>
    </style>
    <style name="small_edit_txt">
        <item name="android:layout_width">220dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/rounded_edittxt</item>
        <item name="android:ems">10</item>
        <item name="android:textAlignment">center</item>

        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/edittxt_hint_grey</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="diagnosis_check_box" parent="normal_txt">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">left</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:layout_marginHorizontal">90dp</item>
        <item name="android:buttonTint">@color/radio_bt</item>
    </style>

    <style name="normal_spinner">
        <item name="android:layout_width">270dp</item>
        <item name="android:layout_height">52dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@color/white</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textAlignment">center</item>
    </style>
    <style name="normal_bt_black">
        <item name="android:layout_width">270dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:backgroundTint">@color/primary_black</item>
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="normal_bt_small_black">
        <item name="android:layout_width">125dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:backgroundTint">@color/primary_black</item>
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="normal_bt_small_red">
        <item name="android:layout_width">125dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:backgroundTint">@color/primary_red</item>
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="normal_bt_peach">
        <item name="android:layout_width">270dp</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:backgroundTint">@color/secondary_peach</item>
        <item name="android:fontFamily">@font/quicksand</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="alert" parent="forgot_password">
        <item name="android:textSize">15dp</item>
        <item name="android:textStyle">bold</item>
    </style>


</resources>